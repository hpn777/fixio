{
  "include": [
    "src"
  ],
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@tsconfig/node18/tsconfig.json",
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "lib": [
      "esnext"
    ],
    "importHelpers": true,
    "sourceMap": true,
    "declarationMap": true,
    "declaration": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "baseUrl": "./src/",
    "removeComments": true,
    "esModuleInterop": true,
    "downlevelIteration": true
  }
}